@model FeatureModel
<!--Mark this form as eam-popup to not track dirty changes-->
<form id="create-feature-popup" class="eam-popup">
    <div class="content">
        <div class="validation-summary-errors none-display" id="feature-error">
        </div>
        <div class="form-horizontal">
            <div class="panel-group">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="form-group">
                            <div class="col-md-3">
                                @Html.BaseEamLabelFor(model => model.ModuleId)
                            </div>
                            <div class="col-md-9">
                                @Html.BaseEamComboBoxFor(model => model.ModuleId, "GetChoices", "Common",
                                        new RouteValueDictionary { { "dbTable", "Module" }, { "dbTextColumn", "Name" }, { "dbValueColumn", "Id" } }, true)
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                @Html.BaseEamLabelFor(model => model.Name)
                            </div>
                            <div class="col-md-9">
                                @Html.BaseEamEditorFor(model => model.Name)
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                @Html.BaseEamLabelFor(model => model.Description)
                            </div>
                            <div class="col-md-9">
                                @Html.BaseEamEditorFor(model => model.Description)
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                @Html.BaseEamLabelFor(model => model.EntityType)
                            </div>
                            <div class="col-md-9">
                                @Html.BaseEamEditorFor(model => model.EntityType)
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                @Html.BaseEamLabelFor(model => model.WorkflowEnabled)
                            </div>
                            <div class="col-md-9">
                                @Html.BaseEamEditorFor(model => model.WorkflowEnabled)
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                @Html.BaseEamLabelFor(model => model.DisplayOrder)
                            </div>
                            <div class="col-md-9">
                                @Html.BaseEamEditorFor(model => model.DisplayOrder)
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>


